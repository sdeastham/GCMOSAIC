mkdir orig_links_dir
cp -p BMosaicAllTestCase_run_comp orig_links_dir
cp -p bScript_for_mosaic_ver1.csh orig_links_dir
cp -p mosiac_error.txt orig_links_dir
cp -p run_mos_cases.csh orig_links_dir

ls -l BMosaicAllTestCase_run_comp
ls -l bScript_for_mosaic_ver1.csh
ls -l mosiac_error.txt
ls -l run_mos_cases.csh

